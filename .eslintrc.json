{
	"extends": ["standard", "standard-react"],
    "plugins": ["standard", "react"],
	"root": true,
    "rules": {
        "jsx-quotes": ["error", "prefer-double"],
        "no-console": ["warn", { "allow": ["info", "warn", "error"] }],
        "react/prop-types": "off"
    },
    "globals": { "fetch": false }
}
